# ===== API KEY SYSTEM CONFIGURATION =====

# Maximum number of API keys per user
MAX_API_KEYS_PER_USER=10

# Maximum number of active sessions per agent per API key
MAX_SESSIONS_PER_AGENT=5

# Default API key settings
API_KEY_LENGTH=32
API_KEY_DEFAULT_EXPIRATION_DAYS=365

# Default session token settings
SESSION_TOKEN_DEFAULT_EXPIRATION_SECONDS=3600
SESSION_MAX_INTERACTIONS_DEFAULT=100

# Security settings
ENFORCE_SESSION_IP_CONSISTENCY=false
ENABLE_IP_RESTRICTIONS=true
ENABLE_DOMAIN_RESTRICTIONS=true

# Rate limiting defaults for API keys
API_KEY_RATE_LIMIT_DEFAULT=60
SESSION_RATE_LIMIT_DEFAULT=60

# Background job settings (for cleanup)
CLEANUP_EXPIRED_SESSIONS_INTERVAL=3600000  # 1 hour in milliseconds
CLEANUP_EXPIRED_TOKENS_ON_STARTUP=true
