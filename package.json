{
  "name": "llm-crafter",
  "version": "0.1.0",
  "description": "A collaborative platform for managing and executing LLM prompts",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest",
    "test:providers": "node test-default-providers.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"*.js\" \"*.json\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.js\" \"*.js\" \"*.json\" \"*.md\"",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "migrate:encrypt-api-keys": "node scripts/migrate-encrypt-api-keys.js",
    "test:encryption": "node scripts/migrate-encrypt-api-keys.js --test-only",
    "verify:api-keys": "node scripts/migrate-encrypt-api-keys.js --verify-only",
    "test:rate-limiting": "node scripts/test-rate-limiting.js",
    "test:password-policy": "node scripts/test-password-policy.js",
    "test:password-api": "node scripts/test-password-api.js"
  },
  "keywords": [
    "llm",
    "ai",
    "prompt-engineering"
  ],
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "express-slow-down": "^3.0.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.5",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "openai": "^4.81.0",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "axios": "^1.11.0",
    "eslint": "^9.18.0",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "vitepress": "^1.6.4"
  }
}
